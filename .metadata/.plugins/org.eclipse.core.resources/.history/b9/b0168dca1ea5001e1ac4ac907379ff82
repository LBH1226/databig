package 산술연산자;

public class Ex01산술연산자 {

	public static void main(String[] args) {

		// 산술연산자
		// / : 몫
		// % : 나머지
		// 정수형 10이라는 데이터를 담는 num1 선언
		int num1 = 10;
		// 실수형 7f라는 데이터를 담는 num2 선언
		float num2 = 7.1f;
		// 정수형과 실수형을 연산하게 되면 자동으로 실수형태로 변환
		System.out.println(num1+num2);
		System.out.println();
		
		
		
		
		
		//short num3 = 5;
		//float num4 = 2.5;
		
		
		
		
		
	}

}
